{
  "listSelector": "div.table-responsive table tbody tr",
  "fields": {
    "title": { "selector": "td a", "attr": "text" },
    "detailUrl": { "selector": "td a", "attr": "href", "absolute": true },
    "seeders": { "selector": "td:nth-child(3)", "attr": "text" },
    "leechers": { "selector": "td:nth-child(4)", "attr": "text" },
    "size": { "selector": "td:nth-child(2)", "attr": "text" }
  },
  "magnet": {
    "fromDetailPage": true,
    "detailMagnetSelector": "a[href^='magnet:?']"
  }
}
